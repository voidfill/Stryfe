{
	"name": "stryfe",
	"version": "0.0.0",
	"description": "A Discord client built with Electron and SolidJS",
	"main": "./out/main/index.js",
	"author": "example.com",
	"homepage": "https://stryfe.app",
	"scripts": {
		"format": "prettier --write .",
		"lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
		"typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
		"typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
		"typecheck": "pnpm run typecheck:node && pnpm run typecheck:web",
		"start": "electron-vite preview",
		"dev": "electron-vite dev",
		"build": "pnpm run typecheck && electron-vite build",
		"postinstall": "npm exec electron-builder install-app-deps",
		"build:win": "electron-vite build && electron-builder --win --config",
		"build:mac": "electron-vite build && electron-builder --mac --config",
		"build:linux": "electron-vite build && electron-builder --linux --config"
	},
	"dependencies": {
		"@electron-toolkit/preload": "^3.0.0",
		"@electron-toolkit/utils": "^3.0.0",
		"@solid-devtools/overlay": "^0.29.3",
		"bindings": "^1.5.0",
		"discord-protos": "^1.0.5",
		"electron-updater": "^5.3.0",
		"erl": "link:native/erl",
		"zlib-sync": "^0.1.9"
	},
	"devDependencies": {
		"@babel/plugin-transform-arrow-functions": "^7.22.5",
		"@electron-toolkit/tsconfig": "^1.0.1",
		"@electron/notarize": "^1.2.4",
		"@solid-primitives/map": "^0.4.9",
		"@solid-primitives/set": "^0.4.9",
		"@solid-primitives/signal-builders": "^0.1.15",
		"@solidjs/router": "^0.12.3",
		"@types/node": "^18.18.2",
		"@typescript-eslint/eslint-plugin": "^5.62.0",
		"@typescript-eslint/parser": "^5.62.0",
		"electron": "^29.1.0",
		"electron-builder": "^24.13.1",
		"electron-vite": "^2.1.0",
		"eslint": "^8.46.0",
		"eslint-config-prettier": "^8.10.0",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-simple-import-sort": "^10.0.0",
		"eslint-plugin-solid": "^0.12.1",
		"eslint-plugin-sort-keys-plus": "^1.3.1",
		"eslint-plugin-typescript-sort-keys": "^2.3.0",
		"nan": "^2.19.0",
		"prettier": "^2.8.8",
		"sass": "^1.64.2",
		"solid-icons": "^1.0.11",
		"solid-js": "^1.8.15",
		"typescript": "^5.4.2",
		"valibot": "^0.20.1",
		"vite": "^5.1.3",
		"vite-plugin-compile-time": "^0.2.1",
		"vite-plugin-solid": "^2.10.1"
	}
}
